{
  "expo": {
    "name": "IJRU Judging",
    "slug": "ijru-judging-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "splash": {
      "image": "./assets/Icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#BCD5FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ijrujudging.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon.png",
        "backgroundColor": "#BCD5FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
